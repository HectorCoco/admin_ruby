<p style="color: green"><%= notice %></p>
<h1>Batches</h1>
<div>
  <p>
    <span>-------------------------------------------------------</span>
  </p>
  <%= link_to "New batch", new_batch_path %>
  <p>
    <span>-------------------------------------------------------</span>
  </p>
</div>
<div id="batches">
  <% @batches.each do |batch| %>
    <%= render batch %>
    <p>
      <%= link_to "Show this batch", batch %>
    </p>
    <span>-------------------------------------------------------</span>
  <% end %>
</div>

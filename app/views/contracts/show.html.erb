<div id="accordion" class="accordion" role="tablist" aria-multiselectable="true">
  <%= render @contract %>
  <br>
  <br>
  <div>
    <%= link_to "Edit this contract", edit_contract_path(@contract) %> ||
    <%= link_to "Back to contracts", contracts_path %>
    <%= button_to "Destroy this contract", @contract, method: :delete, class:"btn btn-info tx-11 pd-y-12 tx-uppercase tx-spacing-2 mb-3"%>
  </div>
</div>
